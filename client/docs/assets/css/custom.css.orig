* {
	font-family: Arial, Helvetica, sans-serif;
}
body {
	overflow-x: hidden!important;
	font-size:15px;
}
.no-pad {
	padding-left: 0!important;
	padding-right: 0!important;
}
.low-pad {
	padding-left: 5px!important;
	padding-right: 5px!important;
}
.p-top {padding-top:20px}
#toprow {
	border-top: 15px solid #442201;
	color: #442201;
	padding-top: 5px;
}
.sareas {
	padding-top: 9px;
	padding-bottom: 9px;
}
.navbar {
	border-radius: 0;
	border: none;
	min-height: 0;
	margin-bottom: 0;
}
h1 {
	color: #ebe8e5;
	height: 65px;
	line-height: 65px;
	margin: 0;
	font-size: 25px;
	text-shadow: 1px 1px 4px rgba(0,0,0,0.39)
}
.homeh1 {
	font-size: 26px;
	color: #442201;
	text-shadow: none;
}
h2 {
	color: #9B6D03;
	border-bottom: 1px solid #442201;
	font-size: 20px;
	font-weight:bold;
}
h3 {font-size:18px;font-weight:bold;}
h4 {font-size:16px;font-weight:bold;}
#pagetitle {background-color:#452301;color:#fff}
a#number {color:#442201;cursor:default;}
a#number:hover {text-decoration:none}
#toprow .lead {margin-bottom:8px}
#sidebar-menu .h3l {
    border: none;
    font-size: 21px;
    font-weight: bold;
    margin-bottom: 14px;}
.h2l {font-size:22px;font-weight:bold;border:none;}
.h1l {margin-top:1em;font-size:26px;font-weight:bold;color:#333;text-shadow:none;height:auto;line-height:normal;}
.h4l {font-weight:bold;font-size:22px;color:#442201}
.large {font-size:18px;}
.landbar {height:4em}
.smaller {font-size:14px}
.disclaimer {font-size:12px}
.navbar-inverse {
	background-color: #442201;
}
.navbar-nav li a {
	color: #FFF!important;
	font-weight: bold;
	text-shadow: 1px 1px 4px rgba(0,0,0,1.00);
}
.navbar-nav li a:hover {
	color: #FFE1C8!important;
}
.dropdown-menu>li>a:hover {
	background: none;
}
#body-outer {
	position: relative;
}
#headerimg {
	position: absolute;
	z-index: -1;
	top: 0;
	width: 100%;
	height: auto;
}
#body-main {
	position: relative;
	z-index: 100;
	background: url(../docs/assets/images/bodybg.gif) top repeat-x #FFF;
	border-radius: 8px 8px 0 0;
	min-height: 300px;
	box-shadow: 0px 0px 50px rgba(68,34,1,.4);
	padding-bottom: 30px;
}
#body-main .homebox a, #body-main #sidebar a {
	color: #442201;
}
.btn-warning {font-weight:bold}
a {color:#a66526;}
.homebox {
	padding-top: 10px;
	padding-bottom: 10px;
	font-weight: bold;
}
#footer-outer {
	background: #442201;
	padding-bottom: 10px;
}
footer {
color: #FFF;
	padding-top: 20px;
	line-height: 15px;
}
footer a {
	color: #FFF!important;
}
.btminfo {
	font-size: 12px;
	line-height: 12px;
}
.btmlinks {
	font-size: 11px;
	line-height: 11px;
}
.btmlinks a, .btmlinks {
	color: #fff!important;
}
#footer-outer footer {font-size:13px;line-height:18px}
legend {
	font-size: 17px;
	font-weight: bold;
	color: #cd9204;
}
.no-decoration p {color:#333}
a.no-decoration {text-decoration:none;}
#sidebar {
	padding-top: 30px;
}
#sidebar-menu {
	background-color: #FFF7EB;
	border-radius: 20px;
	padding: 15px;
	line-height: 20px;
	box-shadow: 2px 2px 5px rgba(0,0,0,0.24);
}
#sidebar-menu li, #sidebar-menu ul {
	margin: 0;
	padding: 0;
	list-style: none;
}
#sidebar-menu h2 {
	color: #442201;
	margin: 0;
}
#sidebar-menu h3 {
	margin-top: 10px;
	margin-bottom: 3px;
	font-size: 15px;
	font-weight: bold;
}
.num {
	font-size: 28px;
	font-weight: bold;
}

/**FORM VALIDATION**/



#captcha {
	display: block;
}
input.error, select.error, textarea.error {
	border: 1px solid red;
}
.errormsg, label.error {
	color: red!important;
	font-size: 13px!important;
}
.red {
	color: red!important;
}
/**MOBILE HOT BUTTONS**/

#mobile-hb-container{
	position:absolute;
	width:100%;
	height:50px;
	padding-right:44px;
}

#mobile-hot-buttons {
	width:150px;/* Must be total width of buttons */
	height:50px;/* Must be height of buttons */
	margin:0 auto;
}

#mobile-hot-buttons a {
	float:left;
}

@media (min-width: 768px) {
	#mobile-hb-container{
		display:none!important;
	}
	
}


/**SIZE SPECIFIC STYLES**/





/**XS**/

@media (max-width: 767px) {
.homebox {
	background: url(../docs/assets/images/bodybg.gif) top repeat-x #FFF;
	text-align: center;
	font-size: 18px;
	padding-left: 0!important;
	padding-right: 0!important;
}
.navbar-nav>li>a {
	border-bottom: 1px solid #000;
	padding-top: 15px;
	padding-bottom: 15px;
}
.num {
	font-size: 15px;
	font-weight: normal;
}
h1 {font-size:17px;line-height:21px;padding-top:10px}
#headerimg {
	width:100%;
	min-height:
}
.lead {font-size:13px}
h1.homeh1 {
	line-height: normal;
	font-size: 26px;
	color: #442201;
	text-shadow: none;
	height:auto;
}
.sareas {padding-bottom:2px}
.h2l {margin-top:12px}
.h1l {text-align:center}
.col-xs-12#pagetitle {float:none}
#footer-outer.landing-pg {
	background: #442201;
	padding-bottom: 10px;}
.v-pad-xs {padding-top:14px}
.xs-uppercase {text-transform:uppercase}
.dropdown-menu .dropdown-menu {padding-left:12px;}
#body-main .homebox a {padding: 9px 0;
    display: inline-block;}
footer a {display:inline-block;padding:7px}
}



